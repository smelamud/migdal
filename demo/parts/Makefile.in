# Makefile for Demo website (parts/ directory)

# @(#) $Id$

OBJECTS = backtrace.php complain.php cs_forum.php cs_normal.php forum.php \
          forums.php login.php menu.php message.php messages.php top.php \
          utils.php
	  
%.php: %.mhtml ../../miparser ../../macros.mhtml ../macros.mhtml
	../../miparser -M ../../macros.mhtml -M ../macros.mhtml $< > $@

all: $(OBJECTS) ;

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

install: all ;

clean:
	-rm $(OBJECTS)
