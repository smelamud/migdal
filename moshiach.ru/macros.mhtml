<< @(#) $Id$ >>
<< Based-on: migdal/macros.mhtml 1.87 (Все, кроме элементов)

<< Errors >>
<macro name='error'>
 <errors>
 <perror id='%id' text='%text' color='%color' span='%span'>
</macro>

<< Elements >>

<decel name='login' file='parts/login.php'>
<decel name='perror' file='parts/utils.php' params='id,text,color,span'
       defaults='color="red" span=0'>
<decel name='top_admin' file='parts/top-admin.php'>
<decel name='topics_admin' file='parts/topics-admin.php' params='ident'>
<decel name='topics_line' params='arrow,text,title,href,count,class,indent,span'
       defaults="arrow='*false' text='' title='' href='' count='' class='item' indent=0 span=1"
       file='parts/topics.php'>
<decel name='topics_table' params='class,span,title'
       defaults="class='' span=1 title=''" file='parts/topics.php'>
<decel name='topics_table_end' file='parts/topics.php'>

<macro name='/topics_table'>
 <topics_table_end>
</macro>

<< HTML shortcuts >>

<macro name='preamble_admin'>
 <stylesheet ident='admin'>
 <head><title>
  Штаб Мошиаха - Администратор<ifne what='$pageTitle'> - $pageTitle</ifne>
 </title></head>
</macro>
