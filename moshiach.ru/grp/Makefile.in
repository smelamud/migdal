# Makefile for GRP definitions

# @(#) $Id$
# Based-on: migdal/grp/Makefile.in 1.2

all: groups.stamp mailings.stamp

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

clean:
	-rm *.php

groups.stamp: groups.conf ../../grpparser
	../../grpparser -C grps.php -L postings.php -p GRP_ -i Posting -m \
		-f grp -a grp $<
	touch $@

mailings.stamp: mailings.conf ../../grpparser
	../../grpparser -C mailtypes.php -L mailings.php -p MAIL_ -i Mailing \
		-f type_id -a mailing $<
	touch $@
