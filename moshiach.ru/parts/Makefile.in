# Makefile for moshiach.ru website (parts/ directory)

# @(#) $Id$

OBJECTS = actual-interview.php advert.php banner.php bottom.php editors.php \
	first-face.php igrot.php links.php menu.php news.php qanda.php search.php \
	spreading.php subscribe.php talking.php top.php topicofday.php updates.php
%.php: %.mhtml ../../miparser ../../macros.mhtml ../macros.mhtml
	../../miparser -M ../../macros.mhtml -M ../macros.mhtml $< > $@

all: $(OBJECTS) ;

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

install: all ;

clean:
	-rm $(OBJECTS)
