# Makefile for moshiach.ru website (parts/ directory)

# @(#) $Id$

OBJECTS = backtrace.php formutils.php grpselect.php login.php postings.php \
	spin.php top-admin.php topics.php topics-admin.php topicselect.php \
	utils.php

%.php: %.mhtml ../../miparser ../../macros.mhtml ../macros.mhtml
	../../miparser -M ../../macros.mhtml -M ../macros.mhtml $< > $@

all: $(OBJECTS) ;

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

clean:
	-rm $(OBJECTS)
