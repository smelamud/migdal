# Makefile for moshiach.ru website (parts/ directory)

# @(#) $Id$

OBJECTS = banner.php menu.php top.php
%.php: %.mhtml ../../miparser ../../macros.mhtml ../macros.mhtml
	../../miparser -M ../../macros.mhtml -M ../macros.mhtml $< > $@

all: $(OBJECTS) ;

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

install: all ;

clean:
	-rm $(OBJECTS)
