! @(#) $Id$

.batcher,.batcher * {
	font-size: 8pt;
}

.batcher A {
	color: %batcher_link;
	text-decoration: none;
	margin-left: 1em;
}

.batcher A:hover {
	text-decoration: underline;
}

.batcher INPUT {
	background-color: %batcher_input_back;
	border: solid 1px %batcher_input_border;
	color: %batcher_input_text;
}
