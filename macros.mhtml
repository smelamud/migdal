  <checkbox_button name='%name' value='%value' iftrue='%iftrue'> %title
 </label>
</macro>

<macro name='radio_button' defaults='peervalue="-1"'>
 <label>
  <ife what="'%id'">
   <if what='(%value)==(%peervalue)'>
    <input type='radio' name='%name' value='%value' checked>%title
   <else>
    <input type='radio' name='%name' value='%value'>%title
   </if>
  <else>
   <if what='(%value)==(%peervalue)'>
    <input type='radio' id='%id' name='%name' value='%value' checked>%title
   <else>
    <input type='radio' id='%id' name='%name' value='%value'>%title
   </if>
  </ife>
 </label>
</macro>

<macro name='radio' defaults='peervalue="-1"'>
 <if what='(%value)==(%peervalue)'>
  <option value='%value' selected>%title
 <else>
  <option value='%value'>%title
 </if>
</macro>

<macro name='select_portion'>
<select name='%name'>
 <array val1='%vals' val2='%value'>
 <iterate_sortarray>
 <for>
  <radio value='$.' peervalue='%value' title='$.'>
 </for>
</select>
</macro>

