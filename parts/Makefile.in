# Makefile for Migdal website (parts/ directory)

# @(#) $Id$

%.php: %.mhtml ../miparser ../macros.mhtml
	../miparser -M ../macros.mhtml $< > $@

all: backtrace.php complain.php cs_forum.php cs_normal.php forum.php \
     forums.php login.php menu.php message.php messages.php top.php \
     utils.php ;

Makefile: Makefile.in ../conf/migdal.conf
	../confmake $^

install: all ;
