<< @(#) $Id$ >>

<int name='complain_id'>
<int name='actid'>

<html>
<stylesheet ident='complain'>
<preamble>
<body>
 <top ident='index'>
 <complain_obj id='$complain_id'>
 <center><table width=95% cellspacing=0 cellpadding=0><tr><td>
  <complain complain='$,'>
 </td></tr></table></center>
 <complain_action_obj id='$actid'>
 <setup>
 <form_table title='Ответ на жалобу'>
  <form method=post action='/actions/complain/action/execute'>
   <hidden name='edittag' value=1>
   <hidden name='okdir' value='$--'>
   <hidden name='faildir' value='$-'>
   <hidden name='actid' value='$actid'>
   <hidden name='complain_id' value='$complain_id'>
   <starinfo>
   <error id='*EECA_NO_ACTION' text='Такого стандартного ответа не существует'>
   <error id='*EECA_NO_COMPLAIN' text='Такой жалобы не существует'>
   <error id='*EECA_NO_EXEC' text='Вам не поручено рассмотрение этой жалобы'>
   <form_complainactionselect title='Действие' name='script_id'
			      value='$,ScriptId' mandatory='*false'>
   <form_editor title='Текст' mandatory='*false' name='text' body='$=,Text'>
   <<help_link href='stotext-help-subs.php'
	      title='Список обозначений, используемых в тексте'>>
   <form_buttons title='Ответить'>
  </form>
 </form_table>
 <bottom>
</body>
</html>
