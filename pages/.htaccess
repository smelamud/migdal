AddDefaultCharset UTF-8
ErrorDocument 404 /404/
ErrorDocument 500 /500/
php_value include_path ".:.."
php_flag magic_quotes_gpc off
php_flag magic_quotes_runtime off
php_flag register_globals off

DirectoryIndex /dispatcher.php
RewriteEngine on
RewriteBase /
RewriteRule ^complains/ - [G]
RewriteRule ^migdal/museum/catalog museum/index.php [L]
RewriteRule ^([^.]+)$ /dispatcher.php
RewriteRule ^([^.]+)\.html$ /dispatcher.php
RewriteCond %{REQUEST_URI} !dispatcher.php
RewriteCond %{REQUEST_URI} !museum/index.php
RewriteRule ^([^.]+)\.php.*$ /dispatcher.php
