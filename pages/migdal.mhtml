<modbits>
<ident name='artid'>
<int name='archive'>

<< Traps >>
<<if what='$artid==$`postings:migdal-library`'>
 <trap text='Migdal library'>
 <reload href='migdal-library.php'>
</if>>
<< Thu May 22 17:29:57 EEST 2003 >>
<<if what='$artid==$`postings:methodic-center`'>
 <trap text='Methodical center'>
 <reload href='methodic-center.php'>
</if>>
<< /Traps >>

<posting_obj id='$artid' grp='*GRP_REVIEWS'
             fields!='*SELECT_GENERAL|*SELECT_LARGE_BODY'>
<log event=review text#='post($,Id)'>

<html>
<stylesheet ident='article'>
<preamble>
<body>
 <top ident='index'
      transref!='$,Id==$`post.migdal` ? "http://english.$siteDomain/" : ""'>
 <table width=100% cellpadding=5><tr valign=top>
  <notprint>
   <td width=150 class='eframe'>
    <topics>
    <ears>
   </td>
  </notprint>
  <td class='article cframe'>
   <if what='!$archive'>
    <assign name='title' value='$,Subject'>
   <else>
    <assign name='title'
            value#='$,Subject (архив от $,Sent@Rj $,Sent@Rk $,Sent@RY)'>
   </ifzero>
   <subtitle title='$title'>
   <article_controls posting='$,' nodiscuss='$englishDomain'>
   <article posting='$,' withvote='*false'>
   <article_controls posting='$,' nodiscuss='$englishDomain'>
  </td>
 </tr></table>
 <bottom>
</body>
</html>
