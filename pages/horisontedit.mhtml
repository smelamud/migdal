<< @(#) $Id$ >>

<string name='host'>

<html>
<preamble>
<body>
 <top ident='index'>
 <horisont_obj host='$host'>
 <setup>
 <form_table title#='Изменить горизонт для $host' mini='*true'>
  <form method=post action='/actions/horisont/modify'>
   <hidden name='edittag' value=1>
   <hidden name='okdir' value='$--'>
   <hidden name='faildir' value='$-'>
   <hidden name='host' value='$host'>
   <starinfo>
   <error id='*EH_NO_MODIFY' text='У вас нет права менять горизонты'>
   <form_edit title='Мы о них' name='we_know' value='$,WeKnow' size=10
	      maxlength=10>
   <form_edit title='Они о нас' name='they_know' value='$,TheyKnow' size=10
	      maxlength=10>
   <form_buttons title='Изменить'>
  </form>
 </form_table>
 <bottom>
</body>
</html>
