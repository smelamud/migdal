<< @(#) $Id$ >>

<element name='banner'>
<div id='logo'>
 <a href='/'>
  <image src='$$style_banner_logo'
	 alt!='!$$englishDomain ? "Башня" : "Tower"'></a>
 <notenglish>
  <a href='/'>
   <image src='$$style_banner_migdal' alt='Мигдаль' id='migdal'></a>
 <else>
  <a href='/'>
   <image src='$$style_banner_migdal_english' alt='Migdal' id='migdal'></a>
 </notenglish>
</div>
</element>

<element name='translation' params='href'>
<ifne what='$href'>
 <english>
  <image src='/pics/lang-ru.gif'>
  <a href='$href' id='translation'>Русская версия</a>
 <else>
  <image src='/pics/lang-us.gif'>
  <a href='$href' id='translation'>English version</a>
 </english>
</ifne>
</element>

<element name='top_web' params='ident,transref'>
<div id='top'>
 <banner>
  <<td align='center' rowspan='2' style="padding-top: 10px">
   <countdown title='дня рождения &#171;Мигдаля&#187;' href='/migdal/events/migdal-birth/18/'
                  day='38' image='/pics/logo-birthday.gif'>
   <notenglish><table cellspacing='5' cellpadding='0'><tr valign='middle'>
    <td align='right'>
     <a href='/migdal/events/other/rainbow-festival-2009/'><asp>
      <image src='/pics/logo-festival.gif'></a>
    </td>
    <td align='left' style='font: 14px Arial,sans-serif'>
     <a href='/migdal/events/other/rainbow-festival-2009/'><asp>
      <asp>Фестиваль &#171;Радуга еврейской Диаспоры&#187;</a>
    </td>
   </table></notenglish>
  </td>>
 <div id='bsd-block'>
  <translation href='$transref'>
  <image src='/pics/bsd-main.gif' alt='БС"Д' id='bsd'>
 </div>
 <login>
</div>
<outer><table width='100%' cellspacing='0' cellpadding='0'><inner>
 <error id='*EL_NO_COOKIES' span='2'
	text='Не работают cookies. Включите поддержку cookies в настройках вашего браузера.'>
</inner></table></outer>
<menu ident='$ident'>
<backtrace>
</element>

<element name='top'>
<notprint>
 <top_web ident='$ident' transref='$transref'>
</notprint>
</element>
