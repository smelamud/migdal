<< @(#) $Id$ >>

<element name='topics_migdal_english'>
<topic_obj name='migdal' id='$`migdal,e`'>
<topics_table title='Migdal' span=2>
 <ispost what='$migdal'>
  <tr><td colspan=3>
   <a href='postingedit.php?redirid=$_&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Add</a>
  </td></tr>
  <tr><td colspan=3>
   <a href='reorder.php?redirid=$_&issue=-1&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Reorder</a>
  </td></tr>
 </ispost>
 <iterate_postinglist grp='*GRP_REVIEWS' limit=0 sort='*SORT_INDEX0'
                      topic_id='$,Id' recursive='*false'>
 <posting_obj id='$`post.migdal,e`'>
 <topics_line time='$,Modified' text='Home' href='/' arrow!="$index=='index'"
              span=2>
 <for>
  <if what='$.Id!=$`post.migdal,e`'>
   <topics_line time='$.Modified' text='$=.Subject' href='$.GrpDetailsHref'
                arrow!='$index==$.Id' class='topics-item ninept' span=2>
  </if>
 </for>
 <posting_last topic_id='$`events,e`' recursive='*false'>
 <topics_line time='$,' text='Events' href='/events/' arrow!="$index=='events'"
              span=2>
 <iterate_postinglist grp='*GRP_TAPE' limit=0 sort='*SORT_SENT'
                      topic_id='$`events,e`' recursive='*false'>
 <for>
  <topics_line time='$.Modified' text='$=.Subject' href='$.GrpDetailsHref'
	       arrow!='$index==$.Id' class='topics-item ninept' indent=1>
 </for>
</topics_table>
</element>
