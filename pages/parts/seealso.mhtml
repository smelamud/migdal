<< @(#) $Id$ >>

<link_types>

<element name='seealso'>
<notprint>
 <outer><div class='seealso'>
  <ife what='$adddir'>
   <assign name='adddir' value#='$~add-cross/'>
  </ife>
  <ife what='$source_name'>
   <assign name='source_name' value='$$LocationInfo.LinkName'>
  </ife>
  <if what='$source_id<=0'>
   <assign name='source_id' value='$$LocationInfo.LinkId'>
  </if>
  <if what="$source_name!='' || $source_id>0">
   <iterate_entry_cross source_name='$source_name' source_id='$source_id'
			link_type='$link_type'>
   <b>$title:</b>
   <inner>
    <moderator>
      <br>
      <a href='${adddir}?source_name=$source_name&source_id=$source_id&link_type=$link_type&redirid=$_'
	 class='edit'>Добавить</a>
    </moderator>
    <outer><ul><inner>
     <assign name='icon' value=''>
     <for>
      <if what="$icon!=$.PeerIcon && $icon!=''">
       <br>&nbsp;
      </if>
      <assign name='icon' value='$.PeerIcon'>
      <li class='seealso-$icon'><a href='$.PeerPath'>$=.PeerSubject</a>
      <moderator>
       <asp>&nbsp;<asp>
       <a href='/actions/cross-entry/delete?id=$.Id&okdir=$#-&faildir=$#-'
	  class='edit'><asp>
	<asp>Удалить</a>
      </moderator>
     </for>
    </inner></ul></outer>
   </inner>
  </if>
 </div></outer>
</notprint>
</element>

<element name='seealso_frame'>
<outer><div class='nframe' style='padding: 5'><inner>
 <seealso link_type='$link_type' title='$title' adddir='$adddir'>
</inner></div></outer>
</element>
