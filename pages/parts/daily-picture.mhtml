<< @(#) $Id$ >>

<element name='daily_picture'>
<table>
 <tr><td>
  <vote_frame id='$picture.Id' color='$$style_cframe_back'
	      image='$$style_cframe_back_pic'>
 </td></tr>
 <if what='$picture.hasSmallImage'>
  <tr><td align=center>
   <posting_image posting='$picture'>
  </td></tr>
 </if>
 <iswrite what='$picture'>
  <tr><td class='eightpt' align=center>
   <editlink posting='$picture'>
  </td></tr>
 </iswrite>
 <ifne what='$picture.TitleHTML'>
  <tr><td class='image-title'>
   $picture.TitleHTML
  </td></tr>
 </ifne>
</table>
</element>
