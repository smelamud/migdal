<< @(#) $Id$ >>

<element name='daily_picture'>
<div>
 <div style='margin-left: auto; margin-right: auto'>
  <vote_panel id='$picture.Id' rating='$picture.Rating'>
 </div>
 <if what='$picture.hasSmallImage'>
  <posting_image posting='$picture' title_large='$picture.TitleLineHTML'
                 rel='$rel'>
 </if>
 <iswrite what='$picture'>
  <div class='eightpt' style='align: center'>
   <editlink posting='$picture'>
  </div>
 </iswrite>
</div>
</element>
