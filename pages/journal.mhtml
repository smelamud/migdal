<< @(#) $Id$ >>

<int name='offset'>

<element name='batcher' params='&list'>
<table width=100%><tr valign=bottom>
 <td align=center><spin list='$list'></td>
</tr></table>
</element>

<macro name='sentprecview'>
 %date@j/&nbsp;%date@H:%date@i:%date@s
</macro>

<html>
<preamble>
<body>
 <top ident='index'>
 <table width=100% cellpadding=5>
  <tr valign=top>
   <td width=150 class='eframe'>
    <topics>
   </td>
   <td class='cframe'>
    <subtitle>
    <admin_users>
     <iterate_horisonts>
     <catalog_table compact='*true'>
      <tr>
       <th class='catalog-cell'>Хост</th>
       <th class='catalog-cell'>Мы о них</th>
       <th class='catalog-cell'>Они о нас</th>
      </tr>
      <for>
       <if what='$;isOdd'>
	<assign name='class' value='catalog-cell-odd'>
       <else>
	<assign name='class' value='catalog-cell'>
       </if>
       <tr>
	<td class='$class' align=left>
	 <a href='/admin/journal/edit-horisont/?host=$#.Host&redirid=$_'><asp>
	  $=.Host</a>
	</td>
	<td class='$class' align=right>$.WeKnow</td>
	<td class='$class' align=right>$.TheyKnow</td>
       </tr>
      </for>
     </catalog_table>
     <iterate_journallist offset='$offset'>
     <batcher list='$;'>
     <catalog_table>
      <tr>
       <th class='catalog-cell'>Id</th>
       <th class='catalog-cell'>Серия</th>
       <th class='catalog-cell'>-&gt; Таблица</th>
       <th class='catalog-cell'>-&gt; Id</th>
       <th class='catalog-cell'>-&gt; &no;</th>
       <th class='catalog-cell'>Запрос</th>
       <th class='catalog-cell'>Дата</th>
      </tr>
      <for>
       <if what='$;isOdd'>
	<assign name='class' value='catalog-cell-odd'>
       <else>
	<assign name='class' value='catalog-cell'>
       </if>
       <tr>
	<td class='$class' align=right>$.Id</td>
	<td class='$class' align=right>$.Seq</td>
	<td class='$class' align=left>$=.ResultTable</td>
	<td class='$class' align=right>$.ResultId</td>
	<td class='$class' align=right>$.ResultVar</td>
	<td class='$class eightpt' align=left>
	 $=.QueryShort
	 <if what='$.QuerySize>10240'>
	  <br><b>(Размер: ${.QuerySizeK}K)</b>
	 </if>
	</td>
	<td class='$class eightpt' align=left><sentprecview date='$.Sent'></td>
       </tr>
      </for>
     </catalog_table>
     <ifnzero what='$;Count'>
      <batcher list='$;'>
     </ifnzero>
    </admin_users>
   </td>
  </tr>
 </table>
 <bottom>
</body>
</html>
