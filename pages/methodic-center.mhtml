<< @(#) $Id$ >>

<log event=review text#='post($`post.migdal.methodology`)'>

<element name='books'>
<html_cache ident='methodic_center_books' depends='postings'>
 <iterate_postinglist grp='*GRP_BOOKS' topic_id='$`migdal.methodology`'
		      recursive='*false' limit=0 sort='*SORT_PSENT'
		      as_guest='*true'>
 <ifnzero what='$;Count'>
  <topics_table title='Методические пособия'>
   <for>
    <topics_line href='$.GrpDetailsHref' text='$=.Heading' class='ninept'>
   </for>
  </topics_table>
 </ifnzero>
</html_cache>
</element>

<html>
 <preamble>

 <top ident='index'>
 <table width=100% cellpadding=5><tr valign=top>
  <notprint>
   <td width=150 class='eframe'>
    <topics>
    <ears>
   </td>
  </notprint>
  <td class='cframe'>
   <table width=100% cellspacing=0 cellpadding=5><tr valign=top>
    <td>
     <posting_obj id='$`post.migdal.methodology`' grp='*GRP_REVIEWS'
                  fields!='*SELECT_GENERAL|*SELECT_LARGE_BODY'>
     <ifnzero what='$,Id'>
      <subtitle>
      <seealso_frame>
      <span class='article'>
       <article_controls posting='$,'>
      </span>
      <notprint>
       <table class='nframe' width=180 align=right
	      cellpadding=5 cellspacing=0
	      style='margin-left: 5px; margin-top: 2ex'>
	<tr><td>
	 <books>
        </td></tr>
       </table>
      </notprint>
      <span class='article'>
       <article posting='$,' withvote='*false'>
       <addtoany>
       <article_controls posting='$,'>
      </span>
     </ifnzero>
    </td>
   </tr></table>
   <rss>
  </td>
 </tr></table>
 <bottom>
</html>
