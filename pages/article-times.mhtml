<< @(#) $Id$ >>

<ident name='postid'>
<posting_obj id='$postid' grp='*GRP_TIMES_ARTICLES' fields='*SELECT_LARGE_BODY'>
<log event=article text#='post($,Id)'>

<element name='times_mark' params='issue'>
<posting_id grp='*GRP_TIMES_COVERS' index1='$issue' topic_id='$`times`'>
<posting_obj id='$,' grp='*GRP_TIMES_COVERS'>
<table align=right class='timesnfo aframe'><tr valign=center>
 <td><a href='/times/$issue/'>
  <image src='/pics/times-small.gif' border=0></a>
 </td>
 <td><a href='/times/$issue/'>No.$,getIssues</a></td>
</tr></table>
</element>

<html>
<stylesheet ident='article'>
<stylesheet ident='timesnfo'>
<preamble>
<body>
 <top ident='times'>
 <table width=100% cellpadding=5><tr valign=top>
  <notprint>
   <td width=150 class='eframe'>
    <topics>
    <ears>
   </td>
  </notprint>
  <td class='article cframe'>
   <article_controls posting='$,'>
   <h1>
    <times_mark issue='$,Index1'>
    <topiclink posting='$,'>
    <br>
    $=,Subject
    <ifne what='$,Author'>
     <div class='author'>$,AuthorHTML</div>
    </ifne>
   </h1>
   <article_full posting='$,'>
   <article_controls posting='$,'>
  </td>
 </tr></table>
 <bottom>
</body>
</html>
