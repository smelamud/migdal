# Makefile for Migdal kernel

# @(#) $Id$

targetdir=$(builddir)/www/js/

DEFAULT_JS=enlarge.js insert-image.js relogin-captcha.js userinfo.js vote.js
SEPARATE_JS=login-verify.js

all:
	-mkdir -p $(targetdir)
	cp jquery*.js $(SEPARATE_JS) $(targetdir)
	cat $(DEFAULT_JS) > $(targetdir)/default.js
	cp .htaccess $(targetdir)

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ $(builddir) > Makefile

clean:
	-rm $(targetdir)/*.js
