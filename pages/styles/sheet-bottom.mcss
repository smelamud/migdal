! @(#) $Id$

#bottom {
	margin-top: 1.5ex;
	color: %bottom;
	font-size: 8pt;
}

#bottom A {
	color: %bottom_link;
	text-decoration: underline;
}

#bottom A.complains-link {
	color: %bottom_complainslink;
	font-size: 9pt;
	font-weight: bold;
}

#bottom A.complains-link:hover {
	color: %bottom_complainslink_hover;
}

#bottom HR {
	clear: both;
	margin-top: 1em;
	border-width: 0;
	border-top: 2px solid %bottom;
	width: 100%;
}
