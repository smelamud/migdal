<< @(#) $Id$ >>

<html>
<stylesheet ident='chat'>
<preamble_tiny>
<body class='chatbottom'>
 <form method=post action='actions/chatpost.php' style='margin: 0; padding: 0'
       target=board name='post'>
 <hidden name='okdir' value='/chatconsole.php?$?'>
 <hidden name='faildir' value='/chatconsole.php?$?'>
 <table style='align: center; margin: 0; padding: 0' cellspacing=0
        cellpadding=0>
  <error id='*ECHP_NO_ADD' span=3
	 text='Добавлять сообщения может только зарегистрированный пользователь'>
  <error id='*ECHP_NO_PERSON' text='Нет такого пользователя' span=3>
  <tr>
   <<td><edit name='personal' value='$personal' size=10 maxlength=30></td>>
   <td><image src='pics/leftinput.gif'></td>
   <td><input class=line type=text autocomplete='off' name='message'
              value='$message' size=68 maxlength=65535></td>
   <td><image src='pics/rightinput.gif'></td>
   <td>&nbsp;</td>
   <td><input type=image src='pics/send.gif'></td>
  </tr>
  <<tr><td align=center><u>лично</u></td></tr>>
 </table>
 </form>
</body>
</html>
