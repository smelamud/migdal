<macro name='preamble_tiny'>
 <conf>
 <assign_global name='wwwDomain' value!="$$userDomain=='www'">
 <assign_global name='englishDomain' value!="$$userDomain=='english'">
 <head>
  <notprint>
   <link rel='alternate' media='print' href='$[print=1]'>
  </notprint>
  <if what="isset($$rss_href) && $$rss_href != ''">
   <link rel="alternate" type="application/rss+xml" title="RSS"
	 href="http://$userDomain.$siteDomain$$rss_href">
  </if>
  <link rel='search' href='/search'>
  <ifne what="'%stylesheet'">
   <link rel='stylesheet' href='%stylesheet'>
  </ife>
  <if what='isset($metaNoIndex) && $metaNoIndex'>
   <meta property="ROBOTS" content="NOINDEX, NOFOLLOW">
  </if>
  <link rel="icon" href="/favicon.ico" type="image/x-icon">
  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
  <notenglish>
   <assign name='siteName' value='Мигдаль'>
  <else>
   <assign name='siteName' value='Migdal'>
  </notenglish>
  <if what="$LocationInfo.Title!='' && $LocationInfo.Title!='Главная'
				    && $LocationInfo.Title!='Main'">
   <meta property="og:type" content="article">
   <assign name='pageTitle' value='$LocationInfo.Title'>
  <else>
   <meta property="og:type" content="website">
   <assign name='pageTitle' value=''>
  </if>
  <meta property="og:site_name" content="$siteName">
  <meta property="og:title" content="$=pageTitle">
  <title><asp>
   <ifne what='$pageTitle'>
    <asp>$=pageTitle |&sp;<asp>
   </ifne>
   $siteName
  <asp></title>
 </head>
</macro>

