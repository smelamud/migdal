<< @(#) $Id$ >>

<modbits>
<ident name='artid'>
<int name='archive'>

<posting_obj id='$artid' grp='*GRP_REVIEWS'
             fields!='*SELECT_GENERAL|*SELECT_LARGE_BODY'>
<log event=review text#='post($,Id)'>

<html>
 <posting_og_image posting='$,'>
 <preamble>

 <if what='$,Id==$`post.migdal`'>
  <assign name='transref' value#='http://english.$siteDomain/'>
 <elif what='$,Id==$`post.migdal.museum`'>
  <assign name='transref' value#='http://english.$siteDomain/museum/'>
 <elif what='$,Id==$`post.museum,e`'>
  <assign name='transref' value#='http://www.$siteDomain/migdal/museum/'>
 <elif what='$,Id==$`post.migdal.migdal-or`'>
  <assign name='transref' value#='http://english.$siteDomain/migdal-or/'>
 <elif what='$,Id==$`post.migdal-or,e`'>
  <assign name='transref' value#='http://www.$siteDomain/migdal/migdal-or/'>
 <else>
  <assign name='transref' value=''>
 </if>
 <top ident='index' transref='$transref'>
 <table width=100% cellpadding=5><tr valign=top>
  <notprint>
   <td width=150 class='eframe'>
    <topics>
    <ears>
   </td>
  </notprint>
  <td class='article cframe'>
   <if what='!$archive'>
    <assign name='title' value='$,Subject'>
   <else>
    <assign name='title'
            value#='$,Subject (архив от $,Sent@Rj $,Sent@Rk $,Sent@RY)'>
   </ifzero>
   <subtitle title='$title'>
   <article_controls posting='$,' nodiscuss='$englishDomain'>
   <article posting='$,' withvote='*false'>
   <addtoany>
   <article_controls posting='$,' nodiscuss='$englishDomain'>
   <rss>
  </td>
 </tr></table>
 <bottom>
</html>
