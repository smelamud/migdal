<< @(#) $Id$ >>

<string name='user_name'>
<string name='group_name'>

<html>
 <preamble>

 <top ident='index'>
 <form_table title='Добавление пользователя в группу'>
  <form method=post action='/actions/group/add'>
   <hidden name='okdir' value='$--'>
   <hidden name='faildir' value='$-'>
   <starinfo>
   <error id='*EGA_NO_ADD' text='Вы не администратор'>
   <error id='*EGA_GROUP_EMPTY' text='Укажите имя группы'>
   <error id='*EGA_NO_GROUP' text='Такой группы не существует'>
   <form_edit title='Группа' mandatory='*true' name='group_name'
	      value='$group_name' size=30 maxlength=30>
   <error id='*EGA_USER_EMPTY' text='Укажите имя пользователя'>
   <error id='*EGA_NO_USER' text='Такого пользователя не существует'>
   <form_edit title='Пользователь' mandatory='*true' name='user_name'
	      value='$user_name' size=30 maxlength=30>
   <form_buttons title='Добавить'>
  </form>
 </form_table>
 <bottom>
</html>
