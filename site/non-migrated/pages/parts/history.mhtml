<< @(#) $Id$ >>

<element name='history'>
<html_cache ident#='history-$topic_id-$current' depends='postings'>
 <iterate_postinglist grp='*GRP_REVIEWS' topic_id='$topic_id' limit=0
		      sort='*SORT_SENT' as_guest='*true'>
 <if what='$;Count>1'>
  <topics_table title='Страницы истории'>
   <for>
    <if what='$.Id!=$today'>
     <topics_line text#='$.Sent@Rj $.Sent@Rk $.Sent@RY' href#='/$.Catalog'
                  arrow!='$.Id==$current' class='topics-item ninept'>
    </if>
   </for>
  </topics_table>
 </if>
</html_cache>
</element>
