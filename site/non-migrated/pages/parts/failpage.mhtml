      <input type=submit value='Send'>
     </notenglish>
     &nbsp;
     <notenglish>
      <input type=reset value='Очистить'>
     <else>
      <input type=reset value='Clear'>
     </notenglish>
    </td></tr>
    <table>
   </form>
 </li>
</ifzero>
</element>
