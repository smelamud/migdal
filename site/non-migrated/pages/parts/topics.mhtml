<< @(#) $Id$ >>

<element name='topics_section'>
<li class='$class'>
 $title
</li>
</element>

<element name='topics_static'>
<if what='$nobullet'>
 <assign name='liclass' value#='topics-nobullet $class'>
<else>
 <assign name='liclass' value#='topics-static $class'>
</if>
<li class='$liclass'>
 $text
</li>
</element>

<element name='topics_subtable'>
<ifne what='$class'>
 <assign name='tableclass' value#='topics $class'>
<else>
 <assign name='tableclass' value='topics'>
</ifne>
<li class='topics-subtable'><ul class='$tableclass'>
</element>

<element name='/topics_subtable'>
</ul></li>
</element>

<element name='upper_level'>
<ife what='$title'>
 <assign name='title' value#='К разделу &lt;&lt;$name&gt;&gt;'>
</ife>
<div class='up-level'>
 <a href='$href'>$=title</a>
</div>
</element>
