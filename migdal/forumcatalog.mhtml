<int name='topic_id'>
<int name='grp'>
<int name='offset'>
<html>
<head>
 <title>Мигдаль - Обсуждения</title>
</head>
<body>
 <top ident='discuss'>
 <topic_obj id='$topic_id'>

 <table width=100%><tr valign=top>
  <grp grp='*GRP_DISCUSS'>
  <td width=120>
   <topics g='$g' topic_id='$topic_id' withpostings='*true' withanswers='*true'
           file='forumcatalog.php'>
  </td>
  <td width=10>&nbsp;</td>
  <td><forumcatalog topic='$topic_id' limit='$userForumCatalogPortion'
                    offset='$offset'></td>
 </tr></table>
 <bottom>
</body>
</html>
