<< @(#) $Id$ >>

<ident name='artid' table='postings'>

<< Traps >>
<if what='$artid>=283 && $artid<=286 || $artid==301'>
 <trap text='Ecology and Torah'>
 <reload href#="book-chapter.php?chapid=$artid">
</if>
<< /Traps >>

<posting_full id='$artid' grp='*GRP_FORUMS'>
<log event=article text#='post($,Id)'>
<pagetitle title='$,Subject'>

<html>
<stylesheet ident='article'>
<preamble>
<body>
 <top ident='index'>
 <table width=100% cellspacing=5 cellpadding=5><tr valign=top>
  <notprint>
   <td width=150 class='eframe'>
    <topics posting='$,'>
   </td>
  </notprint>
  <td class='article cframe'>
   <article_controls posting='$,'>
   <table align=right class='sent'>
    <tr><td>&nbsp;</td></tr>
    <tr><td align=right><sentview date='$,Sent'></td></tr>
    <tr><td align=right><senderlink message='$,'></td></tr>
   </table>
   <h1>
    <topiclink posting='$,'>
    <br>
    $,Subject
    <ifne what='$,Author'>
     <div class='author'>$,HTMLAuthor</div>
    </ifne>
   </h1>
   <article_full posting='$,'>
   <article_controls posting='$,'>
  </td>
 </tr></table>
 <bottom>
</body>
</html>
