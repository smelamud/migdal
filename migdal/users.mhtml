<< @(#) $Id$ >>
<< Based-on: demo/users.mhtml 1.2 >>

<html>
<head>
 <title>Мигдаль - Пользователи</title>
</head>
<body>
 <top ident='users' image='pics/users.gif'>
 <p>
 <table width=100% class='catalog'>
  <tr valign=top>
   <th class='subject'>Ник</th><th>Имя</th><th>Возраст</th>
   <th><img src='pics/tower.gif'></th>
   <th>Адрес</th><th>Последний раз<br>был здесь</th>
  </tr>
  <iterate_userlist>
  <for>
   <if what='$;isOdd'><tr class='odd'><else><tr></if>
    <td class='subject'>
     <a href="useredit.php?editid=$.id&redir=$_">$.Login</a>
    </td>
    <td>$.FullName</td>
    <td align='center'>$.Age</td>
    <td align='center'>$.MigdalStudent</td>
    <td>
     <if what="$.EmailLink!=''">
      <span class='email'>$.EmailLink</span>&nbsp;/&nbsp;
     </if>
     $.ICQStatusImage&nbsp;$.ICQ
    </td>
    <if what='$.isOnline'>
     <td align=center>Сейчас</td>
    <else>
     <if what='$.isTooOld'>
      <td align=center>Очень давно</td>
     <else>
      <td><sentview date='$.LastOnline'></td>
     </if>
    </if>
   </tr>
  </for>
 </table>
 <bottom>
</body>
</html>
