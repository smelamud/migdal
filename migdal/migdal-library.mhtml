<< @(#) $Id$ >>

<posting_full id='$`postings:migdal-library`' grp='*GRP_REVIEW'>
<log event=review text#='post($,Id)'>
<pagetitle title='$,Subject'>
<topictitle title='$,Subject'>

<element name='news'>
<notprint>
 <iterate_postinglist grp='*GRP_LIBRARY_ANNOUNCES' topic_id='$`migdal-library`'
		      recursive='*false' limit=10 sort='*SORT_PSENT'
		      fields='*SELECT_TOPICS'>
 <ifnzero what='$;Count'>
  <table class='nframe' width=180 align=right
	 cellpadding=5 cellspacing=0 style='margin-left: 5px'>
   <tr><td>
    <topics_table title='Новые поступления'>
     <for>
      <identify posting='$.'>
      <topics_line time='$.Sent' href#='$.DetailsHref'
		   text#='$.Author&sp;&lt;&lt;$.Source&gt;&gt;'
		   class='item eightpt'>
     </for>
     <tr><td align=center colspan=2>
      <a href='migdal-library-news.php' class='eightpt'>Все новинки</a>
     </td></tr>
    </topics_table>
   </td></tr>
  </table>
 </ifnzero>
</notprint>
</element>

<html>
<stylesheet ident='article'>
<preamble>
<body>
 <top ident='index'>
 <table width=100% cellpadding=5><tr valign=top>
  <notprint>
   <td width=150 class='eframe'>
    <topics posting='$,'>
   </td>
  </notprint>
  <td class='cframe'>
   <ifnzero what='$,Id'>
    <subtitle>
    <news>
    <span class='article'>
     <article_controls posting='$,'>
     <article posting='$,' withvote='*false'>
     <article_controls posting='$,'>
    </span>
   </ifnzero>
  </td>
 </tr></table>
 <bottom>
</body>
</html>
