<< @(#) $Id$ >>

<html>
<refresh period='$$userChatRefresh' url='$-'>
<preamble>
<body class='chatboard'>
 <update_last_chat>
 <iterate_chat limit='$$userChatPortion'>
 <table cellspacing=0 cellpadding=0>
  <for>
   <tr>
    <td class='time'>[$.Sent@H:$.Sent@i:$.Sent@s]</td>
    <td>&nbsp;</td>
    <td><span class='name'>$.Login:</span> $.HTMLText</td>
   </tr>
  </for>
 </table>
</body>
</html>
