<< @(#) $Id$ >>

<element name='userlink' params='user'>
<if what='!$user.isHidden'>
 <a href='users/$#user.Ident/?redirid=$_'
    onClick="UserInfo($user.Id);return false"
    title='подробнее' class='name'>$user.Login</a><asp>
<else>
 $user.Login<asp>
</if>
<ifne what='$user.ReplyLink'>
 <asp>&nbsp;$user.ReplyLink<image src='$$style_mail_image' border=0></a>
</ife>
</element>

<html>
<refresh period='$$userChatUsersRefresh' url='$-'>
<stylesheet ident='chat'>
<preamble_tiny>
<body class='chatusersframe'>
 <script src='/window.js' language='JavaScript'></script>
 <update_last_chat>
 <iterate_chat_users>
 <table class='chatusers'><tr><td>
  <zone>
   <chat_users_count name='count'>
   <array vals='человек,человека,человек'>
   <plural value='$count'>
   В чате сейчас $count&nbsp;$,
  </zone>
 </td></tr></table>
 <table style='padding-left: 5px'>
  <for>
   <tr>
    <td><userlink user='$.'></td>
   </tr>
  </for>
 </table>
</body>
</html>
