<< Based-on: demo/complainforum.mhtml 1.2 >>

<int name='compid'>
<grps>
<html>
<head>
 <title>Мигдаль - Обсуждение жалобы</title>
</head>
<body>
 <top ident='complains' image='pics/complain.gif'>
 <table width=100% class='complain'>
  <complain_full id='$compid'>
  <if what="$,Display!=''">
   <include name='$,Display'>
   <decel name='complain_subject' params='link'>
   <complain_subject link='$,Link'>
  </if>
  <complain complain='$,' discuss='*false'>
 </table>
 <forums up='$,MessageId' limit='$$userForumPortion'>
 <bottom>
</body>
</html>
