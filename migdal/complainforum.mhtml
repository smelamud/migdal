<< @(#) $Id$ >>

<int name='compid'>
<grps>
<pagetitle title='Обсуждение жалобы'>

<html>
<stylesheet ident='complain'>
<preamble>
<body>
 <top ident='index'>
 <complain_full id='$compid'>
 <div class='cframe' style='padding: 5px'>
  <if what='$,TypeId==*COMPL_POSTING || $,TypeId==*COMPL_AUTO_POSTING'><zone>
   <posting_obj name='posting' id='$,Link' grp='*GRP_NONE'
                fields!='*SELECT_GENERAL|*SELECT_IMAGES|*SELECT_ANSWERS|*SELECT_TOPICS'>
   <if what='$posting.Grp!=*GRP_NONE'>
    <posting posting='$posting' showcomplain='*false'>
    <p>
   </if>
  </zone></if>
  <if what='$,TypeId==*COMPL_FORUM'><zone>
   <forum_obj id='$,Link'>
   <forum forum='$,' showcomplain='*false'>
   <p>
  </zone></if>
  <complain complain='$,' discuss='*false'>
  <p>
  <forums parent_id='$,MessageId' limit='$$userForumPortion'>
 </div>
 <bottom>
</body>
</html>
