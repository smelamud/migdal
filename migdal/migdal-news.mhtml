<< @(#) $Id$ >>

<grps>
<ident name='topic_id'>
<int name='offset'>
<ifzero what='$topic_id'>
 <assign name='topic_id' value='$`general.migdal.news`'>
</ifzero>

<log event=index text#='topic($topic_id) offset=$offset'>

<html>
<preamble>
<body>
 <top ident='index' transref#='http://english.$$siteDomain/events-english.php'>
 <table width=100% cellpadding=5>
  <tr valign=top>
   <td width=150 class='eframe'>
    <topics>
    <ears>
   </td>
   <td class='cframe'>
    <outer><table width=100% cellspacing=0 cellpadding=5><tr valign=top>
     <td><common>
      <subtitle>
      <<outer><div class='nframe' style='padding: 5'><inner>
       <seealso_links topic_id='$topic_id' topic_grp='*GRP_NEWS'
		      title='См. также в разделе ссылок'>
       <seealso_other topic_id='$topic_id' topic_grp='*GRP_NEWS'
		      title='По этой теме в других разделах'>
      </inner></div></outer>>
      <postings_migdal_news topic_id='$topic_id' offset='$offset'>
     </common></td>
     <td width=130 class='nframe'><inner>
      <if what='$topic_id==$`general.migdal.news`'>
       <migdal_news>
       <<events>>
      </if>
     </inner></td>
    </tr></table></outer>
   </td>
  </tr>
 </table>
 <bottom>
</body>
</html>
