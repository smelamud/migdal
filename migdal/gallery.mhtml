<< @(#) $Id$ >>
<< Based-on: migdal/postings.mhtml 1.11 >>

<int name='topic_id'>
<int name='grp'>
<int name='offset'>
<grp ignore='$ignoregrp'>
<grptitle>
<html>
<head>
 <title>Мигдаль - $grptitle</title>
</head>
<body>
 <top ident='index'>
 <topic_obj id='$topic_id'>

 <table width=100%><tr valign=top>
  <td width=120>
   <topics g='$g' topic_id='$topic_id' navi='*true' file='gallery.php'>
  </td>
  <td width=10>&nbsp;</td>
  <td>
   <if what='$topic_id==-1'>
    <gallery_main grp='$grp' cols='$userPictureColumnPortion'>
   <else>
    <gallery grp='$grp' topic='$topic_id' rows='$userPictureRowPortion'
	     cols='$userPictureColumnPortion' offset='$offset'>
   </if>
  </td>
 </tr></table>
 <bottom>
</body>
</html>
