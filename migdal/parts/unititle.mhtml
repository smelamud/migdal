<< @(#) $Id$ >>

<element name='unititle'>
<if what='$topic_id==-1'>
 <assign_global name='topicTitle' value=''>
 <ife what='$chapter'>
  <pagetitle>
 <else>
  <pagetitle title='$chapter'>
 </if>
<else>
 <topic_obj id='$topic_id'>
 <assign_global name='topicTitle' value='$,Name'>
 <ife what='$chapter'>
  <concat name='title' value='$,Name'>
 <else>
  <concat name='title' value='$chapter - $,Name'>
 </if>
 <pagetitle title='$title'>
</if>
</element>
