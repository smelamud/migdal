<< @(#) $Id$ >>

<element name='unititle'>
<grps>
<grptitle>
<if what='$topic_id==-1'>
 <if what='$grp==*GRP_TAPE'>
  <pagetitle>
 <else>
  <pagetitle title='$grptitle'>
 </if>
<else>
 <topic_obj id='$topic_id'>
 <if what='$grp==*GRP_TAPE'>
  <concat name='title' value='$,Name'>
 <else>
  <concat name='title' value='$grptitle --- $,Name'>
 </if>
 <pagetitle title='$title'>
</if>
</element>
