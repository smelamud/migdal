<< @(#) $Id$ >>

<element name='picture_main'>
<table cellspacing=0 cellpadding=0 width=150>
 <tr valign='center'><td class='picture-main' align='center'>
  &nbsp;
  <a href='$href'><asp>
   <ifne what='$image_href'>
    <asp><img border=0 src='$image_href'><asp>
   </ifne>
  <asp></a>
  &nbsp;
 </td></tr>
 <tr><td align='center'>
  <biff time='$biff' small='*true'>
  <ifne what='$title'>
   <a href='$href' title='$=title'>$=name</a>
  <else>
   <a href='$href'>$=name</a>
  </ifne>
 </td></tr>
</table>
</element>
