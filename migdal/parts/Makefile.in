# Makefile for Demo website (parts/ directory)

# @(#) $Id$

OBJECTS = backtrace.php complain.php cs_forum.php cs_normal.php cs_posting.php \
	  forumanswer.php forumcatalog.php forums.php gallery.php login.php \
	  menu.php picture.php posting.php postings.php today.php top.php \
	  topics.php topicselect.php uploader.php utils.php
	  
%.php: %.mhtml ../../miparser ../../macros.mhtml ../macros.mhtml
	../../miparser -M ../../macros.mhtml -M ../macros.mhtml $< > $@

all: $(OBJECTS) ;

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

clean:
	-rm $(OBJECTS)
