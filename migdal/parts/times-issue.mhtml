<< @(#) $Id$ >>

<element name='times_issue'>
<if what='$issue>0'>
 <ifne what='$align'>
  <assign name='al' value#='align=$align'>
 </ifne>
 <if what='$separate'>
  <table width=150 class='mtimes mtsep' $al>
 <else>
  <table width=150 $al>
 </if>
  <if what='$separate'>
   <tr><td class='issuehead'>
    <a href='times.php?issue=$issue'>&lt;&lt;Мигдаль Times&gt;&gt;</a>
   </td></tr>
  </if>
  <tr><td class='issue'>
   No.$issue
  </td></tr>
  <tr><td class='month'>
   $month
  </td></tr>
 </table>
</if>
</element>
