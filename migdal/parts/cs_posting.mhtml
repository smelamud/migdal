<< @(#) $Id$ >>

<element name='complain_subject' params='link'>
<posting_full id='$link'>
<zone><grpdata grp='$,Grp'><grpitemtitle grp='$,Grp'></zone>
<tr><th colspan=2>Жалоба на $grpitemtitle</th></tr>
<tr class='quote'><td colspan=2 align=center>
 <if what='$,Grp!=*GRP_GALLERY'>
  <posting posting='$,' grp='*GRP_FORUMS' complain='*false'>
 <else>
  <picture picture='$,' grp='*GRP_GALLERY' complain='*false'>
 </if>
</td></tr>
</element>
