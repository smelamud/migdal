<< @(#) $Id$ >>

<element name='complain_subject' params='link'>
<grps>
<posting_full id='$link'>
<grpitemtitle grp='$,Grp'>
<tr><th colspan=2>Жалоба на $grpitemtitle</th></tr>
<tr class='quote'><td colspan=2>
 <if what='$,Grp!=*GRP_GALLERY'>
  <posting posting='$,' grp='*GRP_FORUMS' showcomplain='*false'>
 <else>
  <center>
   <picture picture='$,' grp='*GRP_GALLERY' showcomplain='*false'>
  </center>
 </if>
</td></tr>
</element>
