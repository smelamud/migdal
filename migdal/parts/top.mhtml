<< Based-on: demo/parts/top.mhtml 1.2 >>

<element name='Top'>
<!-- begin of Top100 -->
 <a href="http://top100.rambler.ru/top100/">
  <img src="http://counter.rambler.ru/top100.cnt?297350" alt="Rambler's Top100"
       width=1 height=1 border=0></a>
<!-- end of Top100 code -->
<menu ident='$ident' target='$target'>
<table class='state'><tr>
 <td class='topimage'>
  <a href='/index.php'><img src='$$style_banner_name' border=0></a>
  <img src='$$style_banner_logo'>
  <img src='$image'>
 </td>
 <td align=right><login flags='$flags' err='$$err' target='$target'
                        re='$re'></td>
</tr></table>
<table class='state'><tr>
 <td class='style'>
  <zone>
   <concat value='/style.php?redir=$_'>
   <if what="$ident!='style'">
    <at href='$*' target='$target'>Настроить вид</a>
   <else>
    <at href='$*' class='active' target='$target'>Настроить вид</a>
   </if>
  </zone>
 </td>
 <td class='date'><today></td>
</tr></table>
<p>
<backtrace redir='$$redir' target='$target'>
</table>
</element>
