<< @(#) $Id$ >>

<element name='banner' params='target,showimages'>
<if what='$showimages'>
 <table>
  <col width=0* span=2>
  <tr valign=bottom>
   <td>
    <at href='/index.php' target='$target'>
     <image src='$$style_banner_logo'
	    alt!='!$$englishDomain ? "Башня" : "Tower"' border=0></at>
   </td>
   <td align=center>
    <table cellspacing=0 cellpadding=0>
     <notenglish>
      <tr><td><hannukah></td></tr>
      <tr><td>
       <at href='/index.php' target='$target'>
	<image src='$$style_banner_migdal' alt='Мигдаль' border=0></at>
      </td></tr>
     <else>
      <tr><td>
       <at href='/index.php' target='$target'>
	<image src='$$style_banner_migdal_english' alt='Migdal' border=0></at>
      </td></tr>
     </notenglish>
     <tr><td>
      <at href='/index.php' target='$target'>
       <image src='$$style_banner_online' alt='on-line' border=0></at>
     </td></tr>
    </table>
   </td>
  </tr>
 </table>
<else>
 <table style='padding-left: 10px; padding-top: 10px;'>
  <col width=0* span=2>
  <tr valign=bottom>
   <td>
    <at href='/index.php' target='$target'>
     <image src='$$style_banner_migdal' border=0></at>
   </td>
   <td>
    <at href='/index.php' target='$target'>
     <image src='$$style_banner_online' border=0></at>
   </td>
  </tr>
 </table>
</if>
</element>

<element name='translation' params='href'>
<ifne what='$href'>
 <table class='ninept'><tr valign=center>
  <english>
   <td><image src='pics/lang-ru.gif'></td>
   <td><a href='$href'>Русская версия</a>&nbsp;&nbsp;</td>
  <else>
   <td><image src='pics/lang-us.gif'></td>
   <td><a href='$href'>English version</a>&nbsp;&nbsp;</td>
  </english>
 </tr></table>
</ifne>
</element>

<element name='top_web'
         params='ident,no_new,target,showimages,transref,nologin'>
<script src='/window.js' language='JavaScript'></script>
<table width=100% cellspacing=0 cellpadding=0>
 <tr valign=top>
  <td align=left rowspan=2>
   <banner target='$target' showimages='$showimages'>
   <hotlog_count>
   <mailru_count>
   <rambler_count>
  </td>
  <td align=right style='padding-right: 5px; padding-top: 5px'>
   <outer><table cellspacing=0 cellpadding=0><tr valign=top>
    <td><inner><translation href='$transref'></inner></td>
    <td><common><image src='pics/bsd-main.gif' alt='БС"Д'></common></td>
   </tr></table></outer>
  </td>
 </tr>
 <if what='!$nologin'>
  <tr>
   <td align=right style='padding-right: 8px'>
    <login no_new='$no_new' target='$target'>
   </td>
  </tr>
 </if>
 <error id='*EL_NO_COOKIES' span=2
	text='Не работают cookies. Включите поддержку cookies в настройках вашего браузера.'>
</table>
<menu ident='$ident' target='$target'>
<<backtrace target='$target'>>
</element>

<element name='top'>
<notprint>
 <top_web ident='$ident' no_new='$no_new' target='$target'
          showimages='$showimages' transref='$transref' nologin='$nologin'>
</notprint>
</element>
