<< @(#) $Id$ >>

<element name='Menu'>
<table class='menu' width=100%><tr>
 <td>
  <iterate_menu name='menu'>
  <for>
   <if what="$.Ident=='gallery'">
    <posting_last grp='*GRP_GALLERY'>
    <biff time='$,'>
   </if>
   <if what='$.Ident==$ident'>
    <at href='$.Link' class='selected' target='$target'>$.Name</at>
   <else>
    <at href='$.Link' target='$target'>$.Name</at>
   </if>
   <notlast what='$menu'>
    &nbsp;
   </notlast>
  </for>
  <admin_menu>
   &nbsp;
   <at href='menuedit.php' target='$target'>Изменить</at>
  </admin_menu>
 </td>
 <td align=right class='minor'>
  <tune ident='$ident' target='$target'>
 </td>
</tr></table>
</element>
