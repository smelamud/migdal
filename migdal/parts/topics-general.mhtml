<< @(#) $Id$ >>

<element name='navi_kaitana' params='posting'>
<topic_obj name='kaitana' id='kaitana'>

<posting_last topic_id='$kaitana.Id' recursive='*false'>
<topics_line time='$,' href='kaitana.php' title='$kaitana.Description'
	     text='$kaitana.Name' class='item large'>

</element>

<element name='navi_main' params='posting'>
<topics_line href='index.php?topic_id=-1' title='На главную страницу'
	     text='Главная' class='item large'>

</element>

<element name='navi_taglit' params='posting'>
<topic_obj name='taglit' id='taglit'>

<posting_last topic_id='$taglit.Id' recursive='*false'>
<topics_line time='$,' href='taglit.php' title='$taglit.Description'
	     text='$taglit.Name' class='item large'>

</element>

<element name='navi_times' params='posting'>
<topic_obj name='times' id='migdal-times'>

<posting_last topic_id='$times.Id' recursive='*true'>
<topics_line time='$,' href='times.php' title='$times.Description'
	     text='$times.Name' class='item large'>
</element>

<element name='topics'>
<topics_table class='ninept' title='Разделы'>
 <admin_topics>
  <tr><td colspan=2>
   <a href='topicedit.php?redirid=$_' class='edit'>Добавить</a>
   <a href='topicorg.php?redirid=$_' class='edit'>Организовать</a>
  </td></tr>
 </admin_topics>
 <if what='$showchapters'>
  <navi_main posting='$posting'>
  <navi_times posting='$posting'>
  <community>
   <navi_taglit posting='$posting'>
  </community>
  <navi_kaitana posting='$posting'>
 </if>
 <iterate_topiclist grp='$posting.PageGroup' withpostings='$withpostings'
		    withanswers='$withanswers' up='$up' subdomain='$subdomain'>
 <for>
  <topics_line time='$.LastMessage' href#='$posting.PageFile?topic_id=$.Id'
               title='$.Description' text='$.Name' count='$.MessageCount'>
  <admin_topics>
   <topics_line href#='topicedit.php?editid=$.Id&redirid=$_' class='edit'
		text='Изменить'>
  </admin_topics>
 </for>
</topics_table>
</element>
