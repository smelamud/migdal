<< @(#) $Id$ >>

<element name='topiclink_gallery' params='posting,class'>
<assign name='name' value!='$/$posting.TopicId,0/==$`migdal`
			    ? "Мигдаль :: ".$posting.TopicName
			    : $posting.TopicName'>
<at href#='gallery.php?topic_id=$posting.TopicId' class='$class'
    title='$posting.TopicDescription'>$name</at>
</element>

<element name='topiclink_links' params='posting,class'>
<iterate_topic_hierarchy topic_id='$posting.TopicId' root='$`links`'>
<at href#='links.php?topic_id=$posting.TopicId' class='$class'
    title='$posting.TopicDescription'><asp>
 <for>
  <if what='!$;isFirst'>&sp;/&sp;</if>
  $.Name
 </for>
<asp></at>
</element>

<element name='topiclink_times' params='posting,class'>
<at href#='times.php?issue=$posting.Index1' class='$class'
    title='$posting.TopicDescription'>
 <if what='$posting.TopicId!=$`migdal-times`'>
  <asp>Мигдаль Times ::&sp;
 </if>
 $posting.TopicName
</at>
</element>

<element name='topiclink'>
<if what='($posting.Grp & *GRP_GALLERY)!=0'>
 <topiclink_gallery posting='$posting' class='$class'>
<else><if what='($posting.Grp & *GRP_LINKS)!=0'>
 <topiclink_links posting='$posting' class='$class'>
<else><if what='($posting.Grp & *GRP_TIPS)!=0'>
 <at href='tips.php' class='$class' title='$posting.TopicDescription'
     >$posting.TopicName</at>
<else><if what='$/$posting.TopicId,0/==$`migdal-times`'>
 <topiclink_times posting='$posting' class='$class'>
<else><if what='$/$posting.TopicId,0/==$`taglit`'>
 <at href#='taglit-user.php?user_id=$posting.SenderId' class='$class'
     title='$posting.TopicDescription'>$posting.TopicName</at>
<else><if what='$/$posting.TopicId,0/==$`veterans`'>
 <at href#='veterans-user.php?user_id=$posting.SenderId' class='$class'
     title='$posting.TopicDescription'>$posting.TopicName</at>
<else><if what='$/$posting.TopicId,0/==$`migdal`'>
 <if what='$/$posting.TopicId,1/==$`methodics`'>
  <topic_obj id='$`methodics`'>
  <at href='methodics.php' class='$class' title='$,Description'>$,Name</at>
 <else><if what='$/$posting.TopicId,1/==$`migdal-library`'>
  <topic_obj id='$`migdal-library`'>
  <at href='migdal-library.php' class='$class' title='$,Description'>
   Мигдаль :: $,Name</at>
 <else>
  <at href='migdal.php' class='$class'>Мигдаль</at>
 </if></if>
<else><if what='$/$posting.TopicId,0/==$`migdal,e`'>
 <at href='index.php' class='$class'>Migdal</at>
<else>
 <if what='$/$posting.TopicId,1/==$`inet-news`'>
  <topic_obj id='$`inet-news`'>
  <at href='links.php' class='$class' title='$,Description'>$,Name</at>
 <else><if what='$/$posting.TopicId,1/==$`migdal-news`'>
  <topic_obj id='$`migdal-news`'>
  <if what='$posting.TopicId==$`migdal-news`'>
   <at href='migdal-news.php' class='$class' title='$,Description'>$,Name</at>
  <else>
   <at href#='migdal-news.php?topic_id=$posting.TopicId' class='$class'
       title='$posting.TopicDescription'>$,Name :: $posting.TopicName</at>
  </if>
 <else>
  <at href#='index.php?topic_id=$posting.TopicId' class='$class'
      title='$posting.TopicDescription'>$posting.TopicName</at>
 </if></if>
</if></if></if></if></if></if></if></if>
</element>

<element name='editlink'>
<a href="postingedit.php?editid=$posting.Id&topic_id=$posting.TopicId&grp=$posting.Grp&redirid=$_"
   ><notenglish>Изменить<else>Edit</notenglish></a>
</element>

<element name='discusslink'>
<table cellspacing=0 cellpadding=0><tr>
 <td>
  <a href='forum.php?redirid=$_&msgid=$posting.Id'>
   <asp><notenglish>Обсудить<else>Comments</notenglish><asp>
   <ifnzero what='$posting.AnswerCount'>
    <asp>&nbsp;($posting.AnswerCount)<asp>
   </ifnzero>
  <asp></a>
 </td>
 <ifbiff time='$posting.LastAnswer'>
  <td>&nbsp;<image src='pics/discussion-new.gif'></td>
 <else>
  <td>&nbsp;<image src='pics/discussion.gif'></td>
 </ifbiff>
</tr></table>
</element>

<element name='posting'>
<require name='parts/posting-general.php'>
<require name='parts/picture.php'>
<require name='parts/blog-posting.php'>
<require name='parts/posting-book.php'>
<require name='parts/posting-library-announce.php'>
<require name='parts/posting-times.php'>
<require name='parts/posting-times-cover.php'>
<require name='parts/kaitana-picture.php'>
<require name='parts/tip-posting.php'>
<require name='parts/link-posting.php'>
<if what='$posting.TapeCenter'><center></if>
 <exec element='$posting.TapeElement' arg1='$posting' arg2='$showtopic'
       arg3='$showdiscuss' arg4='$showcomplain' arg5='$shorten'>
<if what='$posting.TapeCenter'></center></if>
</element>
