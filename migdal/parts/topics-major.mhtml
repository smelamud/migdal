<< @(#) $Id$ >>

<element name='navi_main' params='ident'>
<topics_line href='index.php' title='На главную страницу'
	     text='Главная' arrow!="$ident=='index'">

</element>

<element name='navi_migdal' params='ident'>
<topic_obj name='migdal' id='$`migdal`'>

<if what='$migdal.UserId==$$userId || $$userModerator'>
 <posting_last topic_id='$`migdal`' recursive='*true'>
 <topics_line time='$,' href='migdal.php' title='$migdal.Description'
	      text='$migdal.Name' arrow!="$ident=='migdal'">
</if>
</element>

<element name='navi_times' params='ident'>
<topic_obj name='times' id='$`migdal-times`'>

<posting_last topic_id='$`migdal-times`' recursive='*true'>
<topics_line time='$,' href='times.php' title='$times.Description'
	     text='$times.Name' arrow!="$ident=='times'">
</element>

<element name='navi_taglit' params='ident'>
<topic_obj name='taglit' id='$`taglit`'>

<posting_last topic_id='$`taglit`' recursive='*false'>
<topics_line time='$,' href='taglit.php' title='$taglit.Description'
	     text='$taglit.Name' arrow!="$ident=='taglit'">
</element>

<element name='navi_kaitana' params='ident'>
<topic_obj name='kaitana' id='$`kaitana`'>

<posting_last topic_id='$`kaitana`' recursive='*false'>
<topics_line time='$,' href='kaitana.php' title='$kaitana.Description'
	     text='$kaitana.Name' arrow!="$ident=='kaitana'">
</element>

<element name='topics_major'>
<topics_table title='Разделы'>
 <admin_topics>
  <tr><td colspan=2>
   <a href='topicorg.php?redirid=$_' class='edit'>Организовать</a>
  </td></tr>
 </admin_topics>
 <navi_main ident='$ident'>
 <navi_migdal ident='$ident'>
 <navi_times ident='$ident'>
 <community>
  <navi_taglit ident='$ident'>
 </community>
 <navi_kaitana ident='$ident'>
</topics_table>
</element>
