<< @(#) $Id$ >>

<element name='at'>
<ifne what='$title'>
 <ifne what='$target'>
  <ifne what='$class'>
   <asp><a href='$href' target='$target' class='$class' title='$title'><asp>
  <else>
   <asp><a href='$href' target='$target' title='$title'><asp>
  </ifne>
 <else>
  <ifne what='$class'>
   <asp><a href='$href' class='$class' title='$title'><asp>
  <else>
   <asp><a href='$href' title='$title'><asp>
  </ifne>
 </ifne>
<else>
 <ifne what='$target'>
  <ifne what='$class'>
   <asp><a href='$href' target='$target' class='$class'><asp>
  <else>
   <asp><a href='$href' target='$target'><asp>
  </ifne>
 <else>
  <ifne what='$class'>
   <asp><a href='$href' class='$class'><asp>
  <else>
   <asp><a href='$href'><asp>
  </ifne>
 </ifne>
</ifne>
</element>

<element name='perror'>
<if what='$$err==$id'>
 <tr><tdspan span=$span>
  <a name='error'></a>
  <span class='error' color='$color'>$text</span>
 </tdspan></tr>
</if>
</element>

<element name='senderlink'>
<if what='$message.isSenderVisible'>
 <a href='userinfo.php?id=$message.SenderId&redirid=$_'
    onClick="UserInfo($message.SenderId);return false"
    title='подробнее' class='name'>$message.Login</a><asp>
<else>
 <span class='name'>$message.Login</span><asp>
</if>
<ifne what='$message.ReplyLink'>
 <asp>&nbsp;$message.ReplyLink<image src='$$style_mail_image' border=0></a>
</ife>
</element>

<element name='lastonline_cell'>
<ifne what='$class'>
 <assign name='cl' value#=" class=$class">
<else>
 <assign name='cl' value=''>
</ifne>
<if what='$user.isOnline'>
 <td align=center$cl>
  <ifnzero what='$user.LastMinutes'>
   <zone>
    <array vals='минуту,минуты,минут'>
    <plural value='$user.LastMinutes'>
    $user.lastMinutes&nbsp;$, назад
   </zone>
  <else>
   Сейчас
  </if>
  <zone>
   <time>
   <biff time='$,'>
  </zone>
 </td>
<else>
 <if what='$user.isTooOld'>
  <td align=center$cl>Очень давно</td>
 <else>
  <td$cl>
   <sentview date='$user.LastOnline'>
   <biff time='$user.LastOnline'>
  </td>
 </if>
</if>
</element>

<element name='navi_sort'>
<if what='$$sort!=$value'>
 <a href='$[sort=$value offset]'>$title</a>
<else>
 <b>$title</b>
</if>
</element>
