<< @(#) $Id$ >>

<element name='history'>
<iterate_postinglist grp='*GRP_REVIEWS' topic_id='$topic_id' limit=0
		     sort='*SORT_SENT'>
<if what='$;Count>1'>
 <topics_table title='Страницы истории'>
  <for>
   <if what='$.Id!=$today'>
    <topics_line time='$.Modified' text#='$.Sent@Rj $.Sent@Rk $.Sent@RY'
		 href#='/$.Catalog' arrow!='$.Id==$current'
		 class='topics-item ninept'>
   </if>
  </for>
 </topics_table>
</if>
</element>
