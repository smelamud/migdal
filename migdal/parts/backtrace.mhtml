<element name='Backtrace'>
<if what="isset($redir) && $redir!=''">
 <tr class='return'><td colspan=2>
  <if what='$$style_return_word'>
   <table><tr align=center>
    <td>
     <at href='$redir' target='$target'>
      <img src='$$style_return_image' border=0></at>
    </td>
    <td><at href='$redir' target='$target'>Вернуться</at></td>
   </tr></table>
  <else>
   <at href='$redir' target='$target'>
    <img src='$$style_return_image' border=0></at>
  </if>
 </td></tr>
</if>
</element>
