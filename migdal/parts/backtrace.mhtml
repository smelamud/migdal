<element name='Backtrace'>
<if what="isset($redir) && $redir!=''">
 <tr class='return'><td colspan=2>
  <if what='$$style_return_word'>
   <table><tr align=center>
    <td><a href='$redir'><img src='$$style_return_image' border=0></a></td>
    <td><a href='$redir'>Вернуться</a></td>
   </tr></table>
  <else>
   <a href='$redir'><img src='$$style_return_image' border=0></a>
  </if>
 </td></tr>
</if>
</element>
