<< @(#) $Id$ >>

<element name='topics_migdal_english'>
<topic_obj name='migdal' id='$`migdal,e`'>
<topics_table>
 <ispost what='$migdal'>
  <tr><td colspan=3>
   <a href='postingedit.php?redirid=$_&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Add</a>
  </td></tr>
  <tr><td colspan=3>
   <a href='reorder.php?redirid=$_&issue=-1&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Reorder</a>
  </td></tr>
 </ispost>
 <iterate_postinglist grp='*GRP_REVIEWS' limit=0 sort='*SORT_INDEX0'
                      topic_id='$,Id' recursive='*false'>
 <posting_obj id='$`postings:migdal,e`'>
 <topics_line time='$,Sent' text='Home' href#='index.php'
              arrow!='$,Id==$posting.Id' indent=0 span=2 class='item'>
 <for>
  <if what='$.Id!=$`postings:migdal,e`'>
   <topics_line time='$.Sent' text='$.Subject' href#='migdal.php?artid=$.Id'
                arrow!='$.Id==$posting.Id' indent=1 span=1 class='item ninept'>
  </if>
 </for>
</topics_table>
</element>
