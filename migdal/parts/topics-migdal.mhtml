<element name='topics_jcc'>
<topic_obj name='jcc' id='$`jcc`'>
<topics_table title='$,Name'>
 <ispost what='$jcc'>
  <tr><td colspan=2>
   <a href='postingedit.php?redirid=$_&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Добавить</a>
  </td></tr>
  <tr><td colspan=2>
   <a href='reorder.php?redirid=$_&issue=-1&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Расставить</a>
  </td></tr>
 </ispost>
 <iterate_postinglist grp='*GRP_REVIEWS' limit=0 sort='*SORT_INDEX0'
                      topic_id='$,Id' recursive='*false'
		      fields='*SELECT_TOPICS'>
 <posting_obj id='$`postings:jcc`' fields='*SELECT_GENERAL'>
 <topics_line time='$,Sent' text='Об общинном центре' href='jcc.php'
              arrow!='$,Id==$posting.Id' class='item ninept'>
 <posting_last topic_id='$`choir`' recursive='*true'>
 <topics_line time='$,' text='Хоровые коллективы'
              href#='migdal.php?artid=$`postings:choir`'
              arrow!="$/$posting.TopicId,2/==$`choir`" class='item eightpt'>
 <posting_last topic_id='$`dances`' recursive='*true'>
 <topics_line time='$,' text='Танцевальные коллективы'
              href#='migdal.php?artid=$`postings:dances`'
              arrow!="$/$posting.TopicId,2/==$`dances`" class='item eightpt'>
 <for>
  <if what='$.Id!=$`postings:jcc`'>
   <topics_line time='$.Sent' text='$.Subject' href#='migdal.php?artid=$.Id'
                arrow!='$.Id==$posting.Id' class='item eightpt'>
  </if>
 </for>
 <posting_last topic_id='$`halom`' recursive='*true'>
 <topics_line time='$,' text='Семейные лагеря' href='halom-main.php'
              arrow!="$/$posting.TopicId,2/==$`halom`" class='item eightpt'>
 <posting_last topic_id='$`kes`' recursive='*true'>
 <topics_line time='$,' text='Клуб Еврейского Студента'
              href#='migdal.php?artid=$`postings:kes`'
              arrow!="$/$posting.TopicId,2/==$`kes`" class='item eightpt'>
</topics_table>
<topics_common_events id='$`kaitana`'>
</element>

