<element name='topics_jcc'>
<upper_level name='Мигдаль' href='/migdal/'>
<topic_obj name='jcc' id='$`migdal.jcc`'>
<topics_table title='$,Subject'>
 <ispost what='$jcc'>
  <tr><td colspan=2>
   <a href='postingedit.php?redirid=$_&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Добавить</a>
  </td></tr>
  <tr><td colspan=2>
   <a href='reorder.php?redirid=$_&issue=-1&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Расставить</a>
  </td></tr>
 </ispost>
 <iterate_postinglist grp='*GRP_REVIEWS' limit=0 sort='*SORT_INDEX0'
                      topic_id='$,Id' recursive='*false'
		      fields='*SELECT_TOPICS'>
 <posting_obj id='$`post.migdal.jcc`' fields='*SELECT_GENERAL'>
 <topics_line time='$,Modified' text='Об общинном центре' href='/migdal/jcc/'
              arrow!="$index=='migdal.jcc'" class='topics-item ninept'>
