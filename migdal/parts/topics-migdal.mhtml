<element name='topics_jcc'>
<upper_level name='Мигдаль' href='/migdal/'>
<topic_obj name='jcc' id='$`migdal.jcc`'>
<topics_table title='$,Subject'>
 <ispost what='$jcc'>
  <tr><td colspan=2>
   <a href='postingedit.php?redirid=$_&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Добавить</a>
  </td></tr>
  <tr><td colspan=2>
   <a href='reorder.php?redirid=$_&issue=-1&topic_id=$,Id&grp=*GRP_REVIEWS'
      class='edit'>Расставить</a>
  </td></tr>
 </ispost>
 <iterate_postinglist grp='*GRP_REVIEWS' limit=0 sort='*SORT_INDEX0'
                      topic_id='$,Id' recursive='*false'
		      fields='*SELECT_TOPICS'>
 <posting_obj id='$`post.migdal.jcc`' fields='*SELECT_GENERAL'>
 <topics_line time='$,Modified' text='Об общинном центре' href='/migdal/jcc/'
              arrow!="$index=='migdal.jcc'" class='topics-item ninept'>
 <posting_last topic_id='$`migdal.jcc.choir`' recursive='*true'>
 <topics_line time='$,' text='Хоровые коллективы' href='/migdal/jcc/choir/'
	      arrow!="$index==$`post.migdal.jcc.choir`"
	      class='topics-item eightpt'>
 <posting_last topic_id='$`migdal.jcc.dances`' recursive='*true'>
 <topics_line time='$,' text='Танцевальные коллективы'
 	      href='/migdal/jcc/dances/'
	      arrow!="$index==$`post.migdal.jcc.dances`"
	      class='topics-item eightpt'>
 <for>
  <if what='$.Id!=$`post.migdal.jcc`'>
   <topics_line time='$.Modified' text='$=.Subject' href#='/$.Catalog'
                arrow!="$index==$.Id" class='topics-item eightpt'>
  </if>
 </for>
 <posting_last topic_id='$`kes`' recursive='*true'>
 <topics_line time='$,' text='Клуб Еврейского Студента'
              href='/migdal/jcc/student/'
	      arrow!="$index==$`post.migdal.jcc.student`"
	      class='topics-item eightpt'>
</topics_table>
<topics_common_events id='$`kaitana`'>
<topics_common_events id='$`halom`'>
</element>

