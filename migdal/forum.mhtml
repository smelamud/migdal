<< @(#) $Id$ >>

<int name='msgid'>
<grps>
<read_posting id='$msgid'>
<posting_full id='$msgid' grp='*GRP_FORUMS'>
<pagetitle title='$,SubjectDesc'>

<html>
<preamble>
<body>
 <top ident='forums'>
 <table width=100%><tr valign=top>
  <td width=120>
   <topics grp='$,Grp' topic_id='$,TopicId' showchapters='*true'>
  </td>
  <td width=5>&nbsp;</td>
  <td>
    <if what='$,Grp!=*GRP_GALLERY'>
     <posting posting='$,' grp='*GRP_FORUMS' discuss='*false'>
    <else>
     <center>
      <picture picture='$,' grp='*GRP_GALLERY' discuss='*false'>
     </center>
    </if>
   <p>
   <forums up='$,MessageId' limit='$$userForumPortion'>
  </td>
 </tr></table>
 <bottom>
</body>
</html>
