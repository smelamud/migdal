! @(#) $Id$

.complains {
	background-color: %complain_border;
}

.complain TD {
	background-color: %complain_back;
}

.complain > TBODY > TR > TD {
	padding-left: 5px;
	padding-right: 5px;
}

.complain .subject {
	font-weight: bold;
}

.complain A {
	text-decoration: none;
}

.complain A:hover {
	text-decoration: underline;
}
