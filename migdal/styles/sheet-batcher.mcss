! @(#) $Id$

.batcher {
	font-size: 8pt;
}

.batcher * {
	font-size: 8pt;
}

.batcher A {
	color: %batcher_link;
	text-decoration: underline;
}

.batcher A.add {
	color: %batcher_normal;
	text-decoration: none;
	margin-left: 1em;
}

.batcher A.add:hover {
	text-decoration: underline;
}

.batcher INPUT {
	border-color: %batcher_input_border;
	border-width: 1px;
	color: %batcher_input_text;
}
