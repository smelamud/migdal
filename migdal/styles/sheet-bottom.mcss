! @(#) $Id$

.bottom {
	margin-top: 1.5ex;
}

.bottom * {
	color: %bottom;
	font-size: 8pt;
}

.bottom A {
	color: %bottom_link;
	text-decoration: underline;
}

.bottom A.big {
	color: %bottom_biglink;
	font-size: 9pt;
	font-weight: bold;
}

.bottom A.big:hover {
	color: %bottom_biglink_hover;
}

.bottom HR {
	margin: 0;
	border-width: 0;
	border-top: 2px solid %bottom;
	width: 100%;
}
