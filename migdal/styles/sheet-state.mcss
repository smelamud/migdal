! @(#) $Id$

.state {
	border-collapse: collapse;
	border-spacing: 0;
	padding: 0;
	width: 100%;
}

.state .login TD {
	padding: 0;
}

.state .login A.name {
	font: bold 11pt %courier;
	text-decoration: underline;
}

.state .login A:hover {
	color: %register_hover;
}

.state .login INPUT.line {
	background-color: %login_input_back;
	border: solid 1px %login_input_border;
	color: %login_text;
}
