# Makefile for Migdal website

# @(#) $Id$

STYLES = default contrast test
OBJECTS = $(patsubst %,style-%.css,$(STYLES)) $(patsubst %,style-%.php,$(STYLES))

style-%.php: style.mcss ../../cssparser
	../../cssparser -p -s $* $< > $@

style-%.css: style.mcss ../../cssparser
	../../cssparser -s $* $< > $@

all: $(OBJECTS)

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

clean:
	-rm $(OBJECTS)
