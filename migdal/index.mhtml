<< Based-on: demo/postings.mhtml 1.8 >>

<int name='topic_id'>
<int name='offset'>
<reloadnot why='$topic_id' var='topic_id' value='-1'>
<reloadnot why='$grp' var='grp' value='*GRP_TAPE'>
<grp>
<grptitle>
<grppic>
<html>
<head>
 <title>Мигдаль<if what='$g.Effective!=*GRP_TAPE'> - $grptitle</if></title>
</head>
<body>
 <top ident='index' image='$grppic'>
 <topic_obj id='$topic_id'>

 <table width=100%><tr valign=top>
  <td width=120><topics g='$g' topic_id='$topic_id' navi='*true'></td>
  <td width=20>&nbsp;</td>
  <td><postings grp='$grp' topic='$topic_id' limit='$userMsgPortion'
                offset='$offset' title='$grptitle'></td>
 </tr></table>
 <bottom>
</body>
</html>
