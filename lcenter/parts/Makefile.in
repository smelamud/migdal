# Makefile for Migdal website (parts/ directory)

# @(#) $Id$

OBJECTS = bottom.php formutils.php grpselect.php langselect.php spin.php \
	subtitle.php top.php topics-admin.php topicselect.php topicsutils.php \
	uploader.php utils.php
	  
%.php: %.mhtml ../../miparser ../../macros.mhtml ../macros.mhtml
	../../miparser -M ../../macros.mhtml -M ../macros.mhtml $< > $@

all: $(OBJECTS) ;

Makefile: Makefile.in ../../conf/migdal.conf
	../../confmake $^ > Makefile

clean:
	-rm $(OBJECTS)
