<< @(#) $Id$ >>

<element name='banners'>
<array name='urls'
       val1='http://www.anybrowser.org/campaign/'
       val2='http://www.migdal.ru/'>
<array name='pics'
       val1='pics/anybrowser.png'
       val2='http://www.migdal.ru/pics/banner-88x31-2.gif'>
<array name='alts'
       val1='Use&sp;any&sp;browser!'
       val2='Мигдаль'>
<<assign name='url' value!='$urls[4]'>
<assign name='pic' value!='$pics[4]'>
<assign name='alt' value!='$alts[4]'>
<a href='$url' target=_blank>
 <img src='$pic' width=88 height=31 border=0 alt='$alt'></a>>
<iterate_random_seq n=3 min=1 max=2>
<for>
 <assign name='url' value!='$urls[$. - 1]'>
 <assign name='pic' value!='$pics[$. - 1]'>
 <assign name='alt' value!='$alts[$. - 1]'>
 <a href='$url' target=_blank>
  <img src='$pic' width=88 height=31 border=0 alt='$alt'></a>
</for>
&nbsp;
&nbsp;
</element>
