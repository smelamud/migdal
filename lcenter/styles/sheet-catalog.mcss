! @(#) $Id$

.catalog {
	border-collapse: collapse;
	border-spacing: 0;
	border: solid 1px %catalog_border;
}

.catalog TT,.catalog PRE {
	font: 10pt %courier;
}

.catalog TH {
	font-size: 9pt;
	background-color: %catalog_header;
	color: %catalog_header_text;
	vertical-align: middle;
	text-align: center;
}

.catalog TD,.catalog TH {
	border-right: solid 1px %catalog_border;
	padding: 3px;
}

.catalog TR {
	background-color: %catalog_even;
}

.catalog TR.odd {
	background-color: %catalog_odd;
}
